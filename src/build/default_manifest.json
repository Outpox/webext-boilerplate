{
    "description": "",
    "manifest_version": 2,
    "name": "mynewextenstion",
    "version": "1.0",
    "author": "Guillaume 'Outpox' Migeon",
    "icons": {},
    "permissions": [""],
    "browser_action": {
        "default_popup": "html/popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "js/browser-polyfill.min.js",
                "js/content.js"
            ]
        }
    ],
    "background": {
        "scripts": [
            "js/browser-polyfill.min.js",
            "js/background.js"
        ]
    }
}